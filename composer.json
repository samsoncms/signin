{
  "name": "samsoncms/signin",
  "description": "SamsonCMS signin application",
  "author": "Olexandr Nazarenko <nazarenko@samsonos.com>",
  "require": {
    "samsoncms/application": "*",
    "samsonos/php_email": "*",
    "samsonos/php_url": "*",
    "samsonos/social_core": "*",
    "samsonos/social_email": "*",
    "samsonos/js_core": "*"
  },
  "require-dev": {
    "behat/behat": "3.*@stable",
    "behat/mink": "1.6.*@stable",
    "behat/mink-extension": "@stable",
    "behat/mink-selenium2-driver": "@stable"
  },
  "minimum-stability": "dev",
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "autoload": {
    "classmap": ["src/"],
    "psr-4": {
      "samsoncms\\app\\signin\\": "src/"
    }
  },
  "config": {
    "github-protocols": ["https"]
  }
}
